{
  "name": "elementify-blocks",
  "version": "1.0.0",
  "description": "Elementify Blockss plugin packages",
  "main": "main.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode production --progress",
    "start": "cross-env NODE_ENV=development webpack --watch --mode development --progress",
    "clean": "rm -rf assets/build/*",
    "svg": "svgr -d assets/src/js/icons assets/src/icons",
    "lint:php": "vendor/bin/phpcs",
    "lint:php:fix": "vendor/bin/phpcbf",
    "lint": "eslint assets/src/js/*",
    "lint:fix": "eslint --fix assets/src/js/*",
    "lint-css": "stylelint 'assets/src/sass/**/*.scss'",
    "lint-css:fix": "npm run lint-css -- --fix",
    "precommit": "npm run lint:php && npm run lint-css:fix && npm run lint:fix && npm run prod",
    "packages-update": "wp-scripts packages-update",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "keywords": [
    "wordpress",
    "themes"
  ],
  "author": "Elementify Themes",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@svgr/cli": "^6.2.1",
    "@tailwindcss/forms": "^0.5.7",
    "@wordpress/base-styles": "^4.3.0",
    "@wordpress/block-editor": "^8.4.0",
    "@wordpress/blocks": "^11.4.0",
    "@wordpress/components": "^19.7.0",
    "@wordpress/compose": "^5.3.0",
    "@wordpress/data": "^6.5.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^3.4.1",
    "@wordpress/element": "^4.3.0",
    "@wordpress/eslint-plugin": "^11.1.0",
    "@wordpress/hooks": "^3.5.0",
    "@wordpress/i18n": "^4.56.0",
    "@wordpress/icons": "^8.1.0",
    "@wordpress/server-side-render": "^3.3.0",
    "babel-loader": "^8.2.4",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.12.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^38.1.6",
    "file-loader": "^6.2.0",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "node-sass": "^7.0.1",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "precss": "^4.0.0",
    "sass": "^1.74.1",
    "sass-loader": "^12.6.0",
    "sass-mq": "^6.0.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.6.1",
    "stylelint-config-wordpress": "^17.0.0",
    "stylelint-webpack-plugin": "^3.2.0",
    "tailwindcss": "^3.4.3",
    "terser-webpack-plugin": "^5.3.10",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.19",
    "@wordpress/api-fetch": "^6.53.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.1",
    "react-router-dom": "^6.23.0"
  }
}
